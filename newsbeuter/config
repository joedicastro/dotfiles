datetime-format                         "%d %b"
delete-read-articles-on-quit            no
dialogs-title-format                    "%N %V - Di√°logos"
display-article-progress                yes
download-full-page                      yes
download-path                           "~/"
download-retries                        1
download-timeout                        30
error-log                               "~/.newsbeuter/error.log"
external-url-viewer                     ""
feed-sort-order                         "none-desc"
feedlist-format                         "%4i %n %11u %t"
feedlist-title-format                   "%N %V - Tus feeds (%u no leidos, %t total)%?T? - etiqueta `%T'&?"
filebrowser-title-format                "%N %V - %?O?Abrir arcivo&Salvar archivo? - %f"
googlereader-flag-share                 ""
googlereader-flag-star                  ""
googlereader-login                      ""
googlereader-min-items                  20
googlereader-password                   ""
googlereader-passwordfile               ""
googlereader-show-special-feeds         true
goto-first-unread                       true
goto-next-feed                          yes
help-title-format                       "%N %V - Ayuda"
history-limit                           50
http-auth-method                        any
html-renderer                           "internal"
ignore-mode                             "display"
itemview-title-format                   "%N %V - Articulo '%T'"
keep-articles-days                      15
mark-as-read-on-hover                   false
max-download-speed                      0
max-downloads                           1
max-items                               25
notify-always                           yes
notify-beep                             no
notify-format                           "%d articulos nuevos, (%n articulos no leidos, %f feeds no leidos)"
notify-program                          "~/.newsbeuter/notify.py"
notify-screen                           yes
notify-xterm                            no
opml-url                                ""
pager                                   "internal"
player                                   ""
podcast-auto-enqueue                    no
prepopulate-query-feeds                 no
proxy                                   ""
proxy-auth                              ""
