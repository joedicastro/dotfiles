#!/bin/sh

setxkbmap -layout us,es -variant altgr-intl, -option grp:menu_toggle -option grp_led:scroll -option ctrl:nocaps
xrdb -merge ~/.Xresources
mpd ~/.mpd/mpd.conf
~/.bin/xcape
udiskie &
autocutsel -fork &
autocutsel -selection PRIMARY -fork &
unclutter &
compton -b --config ~/.compton.conf 
urxvtd -q -o -f &
exec awesome
